@model List<K191273_Qs4.Models.MarketSummary>

@{
    ViewBag.Title = "All";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="mycontainer">
    <h1>Market Summary</h1>
    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">Script</th>
                <th scope="col">Price</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var element in Model)
            {
                <tr>
                    <td scope="col">@Html.DisplayFor(m => element.Script)</td>
                    <td scope="col">@Html.DisplayFor(m => element.Price)</td>
                </tr>
            }
        </tbody>

    </table>
</div>
